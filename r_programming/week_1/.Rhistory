## the location of the CSV files
## 'pollutant' is a character vector of length 1 indicating
## the name of the pollutant for which we will calculate the
## mean; either "sulfate" or "nitrate".
## 'id' is an integer vector indicating the monitor ID numbers
## to be used
## Return the mean of the pollutant across all monitors list
## in the 'id' vector (ignoring NA values)
wd <- paste(getwd(), directory, sep="/")
data_files <- list.files(wd, pattern="*.csv")
my_file <- data_files[id[1]]
print(my_file)
master_file <- read.csv(paste(wd, my_file, sep="/"))
if(length(id) > 1) {
for (i in id[2:length(id)]) {
next_file <- read.csv(paste(wd, data_files[id[i]], sep="/"))
master_file <- rbind(master_file, next_file)
}
}
mean(master_file[[pollutant]], na.rm=TRUE)
}
